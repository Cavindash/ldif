<?xml version="1.0" encoding="utf-8" ?>
<Sieve xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="../../xsd/SieveSpecificationLanguage.xsd" >

    <Prefixes>
        <Prefix id="rdf" namespace="http://www.w3.org/1999/02/22-rdf-syntax-ns#" />
        <Prefix id="rdfs" namespace="http://www.w3.org/2000/01/rdf-schema#" />
        <Prefix id="owl" namespace="http://www.w3.org/2002/07/owl#" />
        <Prefix id="mo" namespace="http://purl.org/ontology/mo/" />
        <Prefix id="dbpedia" namespace="http://dbpedia.org/resource/" />
    </Prefixes>

    <Fusion>

        <Class name="dbpedia:City">
            <Property name="rdfs:label">
                <FusionFunction class="KeepSingleValueByQualityScore" indicator="ldif:lastUpdate"/>
            </Property>
        </Class>

    </Fusion>

</Sieve>
