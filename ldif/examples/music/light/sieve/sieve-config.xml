<?xml version="1.0" encoding="utf-8"  ?>
<Sieve xmlns="http://ldif.wbsg.de/">
    <Prefixes>
        <Prefix id="ldif" namespace="http://ldif.wbsg.de/"/>
        <Prefix id="sieve" namespace="http://sieve.wbsg.de/vocab/"/>
        <Prefix id="dbpedia-owl" namespace="http://dbpedia.org/ontology/"/>
        <Prefix id="dbpedia" namespace="http://dbpedia.org/resource/"/>
        <Prefix id="mo" namespace="http://purl.org/ontology/mo/" />
        <Prefix id="rdfs" namespace="http://www.w3.org/2000/01/rdf-schema#" />
    </Prefixes>

    <QualityAssessment>
        <AssessmentMetric id="sieve:reputation">
            <ScoringFunction class="ScoredList">
                <Param name="list" value="http://ldif.wbsg.de/graph#dbpedia.3 http://ldif.wbsg.de/graph#dbpedia.2 http://ldif.wbsg.de/graph#dbpedia.1 http://rdf.freebase.com/rdf/en/bob_marley http://ldif.wbsg.de/graph#musicbrainz.3 http://ldif.wbsg.de/graph#bbc.2"/>
            </ScoringFunction>
        </AssessmentMetric>
    </QualityAssessment>

    <Fusion>
        <Class name="mo:MusicArtist">
            <Property name="rdfs:label">
                <FusionFunction class="KeepFirst" metric="sieve:reputation"/>
            </Property>
        </Class>
        <Class name="mo:Record">
            <Property name="rdfs:label">
                <FusionFunction class="KeepFirst" metric="sieve:reputation"/>
            </Property>
        </Class>
    </Fusion>

</Sieve>