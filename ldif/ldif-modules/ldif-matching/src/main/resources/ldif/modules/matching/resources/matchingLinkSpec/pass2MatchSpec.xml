                        <!--</TransformInput>-->
                        <!--<TransformInput function="tokenize">-->
                            <Input path="?b\rdfs:subClass/rdfs:label" />
                        <!--</TransformInput>-->
                    </Compare>
                <!--</Aggregate>-->
            </LinkageRule>

            <Filter limit="1"/>
        </Interlink>
