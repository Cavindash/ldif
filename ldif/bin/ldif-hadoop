#!/bin/bash
basedir=${0%/*}
java -server -Xmx4G -Xms256M -Dlogback.configurationFile=$basedir/../resources/logback.xml -cp $basedir/../:$basedir/../ldif-hadoop-executor/target/ldif-hadoop-exe-0.3.1-jar-with-dependencies.jar ldif.hadoop.HadoopIntegrationJob $basedir/../examples/life-science/hadoopIntegrationJob.xml $*