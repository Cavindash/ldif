<?xml version="1.0" encoding="utf-8"?>
<integrationJob xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:schemaLocation="http://www4.wiwiss.fu-berlin.de/ldif/ ../xsd/IntegrationConfig.xsd"
                xmlns="http://www4.wiwiss.fu-berlin.de/ldif/">
    <properties>integration.properties</properties>
    <sources>
        <source>dumps     </source>
    </sources>
    <outputs>
        <output>
            <sparql>
                <endpointURI>http://host/sparql</endpointURI>
                <queryParameter>update</queryParameter>
                <sparqlVersion>1.1</sparqlVersion>
                <useDirectPost>false</useDirectPost>
                <user>usr</user>
                <password>pwd</password>
            </sparql>
            <phase>
                silk
            </phase>
        </output>
        <output>
            <file>output.nq</file>
        </output>
    </outputs>
</integrationJob>
